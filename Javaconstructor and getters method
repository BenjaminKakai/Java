public class Person {
    private int personAge;

    public Person (int age) {
        this.personAge = age;
    }

    public int getPersonAge() {
        return personAge;
    }

    public static void main (String[] args) {
        Person person  = new Person(21);

        System.out.println("Benjamin Kakai is " + person.getPersonAge() + " years old.");
    }
}
